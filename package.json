{
  "name": "eslint-code-explorer",
  "version": "0.1.0",
  "private": true,
  "description": "A tool that visualizes JS and TS code structure and scope using ESLint's scope analysis.",
  "repository": "eslint/code-explorer",
  "funding": "https://opencollective.com/eslint",
  "bugs": "https://github.com/eslint/code-explorer/issues",
  "type": "module",
  "scripts": {
    "build": "vite build",
    "build:preview": "vite preview",
    "build:readme": "node tools/update-readme.js",
    "start": "vite",
    "build:types": "tsc",
    "lint": "pnpm run \"lint:js\"",
    "lint:fix": "pnpm run \"/^lint:fix:.*/\"",
    "lint:js": "eslint --max-warnings 0 .",
    "lint:fix:js": "eslint --max-warnings 0 . --fix",
    "fmt": "prettier --check .",
    "fmt:fix": "prettier --write .",
    "test:e2e": "PW_TEST_CONNECT_WS_ENDPOINT=ws://127.0.0.1:3000/ playwright test",
    "test:e2e:update": "pnpm run test:e2e --update-snapshots"
  },
  "lint-staged": {
    "**/*.{ts,tsx,jsx,js}": [
      "eslint --fix",
      "prettier --write"
    ],
    "!(*.(js|jsx|ts|tsx))": "prettier --write --ignore-unknown"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "engines": {
    "node": ">= 20"
  },
  "dependencies": {
    "@codemirror/lang-css": "^6.3.1",
    "@codemirror/lang-javascript": "^6.2.3",
    "@codemirror/lang-json": "^6.0.1",
    "@codemirror/lang-markdown": "^6.3.2",
    "@codemirror/language": "^6.11.0",
    "@codemirror/state": "^6.5.2",
    "@codemirror/view": "^6.36.5",
    "@eslint/css": "^0.6.0",
    "@eslint/js": "^9.24.0",
    "@eslint/json": "^0.11.0",
    "@eslint/markdown": "^6.3.0",
    "@lezer/highlight": "^1.2.1",
    "@radix-ui/react-accordion": "^1.2.4",
    "@radix-ui/react-dialog": "^1.1.7",
    "@radix-ui/react-dropdown-menu": "^2.1.7",
    "@radix-ui/react-label": "^2.1.3",
    "@radix-ui/react-popover": "^1.1.7",
    "@radix-ui/react-select": "^2.1.7",
    "@radix-ui/react-slot": "^1.2.0",
    "@radix-ui/react-switch": "^1.1.4",
    "@radix-ui/react-toast": "^1.2.7",
    "@radix-ui/react-toggle": "^1.1.3",
    "@radix-ui/react-toggle-group": "^1.1.3",
    "@uiw/react-codemirror": "^4.23.10",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "codemirror": "^6.0.1",
    "eslint-linter-browserify": "^9.24.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-scope": "^8.3.0",
    "espree": "^10.3.0",
    "esquery": "^1.6.0",
    "graphviz-react": "^1.2.5",
    "lucide-react": "^0.487.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "tailwind-merge": "^3.2.0",
    "tailwindcss-animate": "^1.0.7",
    "use-debounced-effect": "^2.0.1",
    "zustand": "^5.0.3"
  },
  "devDependencies": {
    "@playwright/test": "^1.51.1",
    "@types/eslint-scope": "^3.7.7",
    "@types/espree": "^10.1.0",
    "@types/esquery": "^1.5.4",
    "@types/estree": "^1.0.7",
    "@types/node": "^22.14.0",
    "@types/react": "^19.1.0",
    "@types/react-dom": "^19.1.1",
    "@vitejs/plugin-react": "^4.3.4",
    "eslint": "^9.24.0",
    "globals": "^16.0.0",
    "got": "^14.4.7",
    "lint-staged": "^15.5.0",
    "postcss": "^8.5.3",
    "postcss-load-config": "^6.0.1",
    "prettier": "^3.5.3",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "react-resizable-panels": "^2.1.7",
    "tailwindcss": "^3",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.29.1",
    "vite": "^6.2.5",
    "yorkie": "^2.0.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "packageManager": "pnpm@10.8.0+sha512.0e82714d1b5b43c74610193cb20734897c1d00de89d0e18420aebc5977fa13d780a9cb05734624e81ebd81cc876cd464794850641c48b9544326b5622ca29971"
}
